keywordc=teal
keywordb=$(formbb)$(keywordc)$(formba)
keyworda=$(forma)

=keyword

keyworddarkc=darkblue
keyworddarkb=$(formbb)$(keyworddarkc)$(formba)
keyworddarka=$(forma)

=midword

midwordc=darkred
midwordb=$(formbb)$(midwordc)$(formba)
midworda=$(forma)

midwordlightc=red
midwordlightb=$(formbb)$(midwordlightc)$(formba)
midwordlighta=$(forma)

commentc=green
commentvarc=darkblue
commentvarb=$(formbb)$(commentvarc)$(formba)
commentvara=$(forma)

stringc=red

nestcomc=green

numberc=blue

fpnumberc=darkblue

.c_1ext
.cfrule
